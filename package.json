  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/blaircalderwood/serverless-scaffolder"
  },
  "main": "generators/index.js",
  "keywords": [
    "serverless",
    "AWS",
    "Lambda",
    "API Gateway",
    "scaffolder",
    "yeoman",
    "generator",
    "node",
    "javascript",
    "jest",
    "webpack",
    "code-coverage",
    "yeoman-generator"
  ],
  "devDependencies": {
    "coveralls": "^3.0.5",
    "eslint": "^6.0.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-config-xo": "^0.26.0",
    "eslint-plugin-prettier": "^3.1.0",
    "husky": "^3.0.1",
    "lint-staged": "^9.2.0",
    "prettier": "^1.18.2",
    "yeoman-assert": "^3.1.0",
    "yeoman-test": "^1.7.0"
  },
  "engines": {
    "npm": ">= 4.0.0"
  },
  "dependencies": {
    "chalk": "^2.1.0",
    "lodash": "^4.17.15",
    "yeoman-generator": "^2.0.1",
    "yosay": "^2.0.1"
  },
  "jest": {
